  OWNER TO mobisaude_pg_user;

-- Table: historico

-- DROP TABLE historico;

CREATE TABLE historico
(
  id integer NOT NULL,
  id_log integer,
  prestadora character varying,
  qtd_registros integer,
  CONSTRAINT pk_historico PRIMARY KEY (id)
)
WITH (
  OIDS=FALSE
);
ALTER TABLE historico
  OWNER TO mobisaude_pg_user;

-- Table: log

-- DROP TABLE log;

CREATE TABLE log
(
  id_log integer NOT NULL,
  data_hora_inicio timestamp without time zone,
  data_hora_fim timestamp without time zone,
  evento character varying(20),
  observacao character varying,
  CONSTRAINT pk_log PRIMARY KEY (id_log)
)
WITH (
  OIDS=FALSE
);
ALTER TABLE log
  OWNER TO mobisaude_pg_user;

-- Table: relatorio_erbs

-- DROP TABLE relatorio_erbs;

CREATE TABLE relatorio_erbs
(
  uf character varying(2),
  cod_municipio_ibge character varying NOT NULL,
  municipio character varying,
  prestadora character varying,
  nome_fantasia character varying,
  tecnologia_2g boolean,
  tecnologia_3g boolean,
  tecnologia_4g boolean,
  latitude_stel double precision,
  longitude_stel double precision,
  id_relatorio_erbs integer NOT NULL,
  CONSTRAINT pk_relatorio_erbs PRIMARY KEY (id_relatorio_erbs)
)
WITH (
  OIDS=FALSE
);
ALTER TABLE relatorio_erbs
  OWNER TO mobisaude_pg_user;

-- Index: idx_relatorio_erbs_cod_mun

-- DROP INDEX idx_relatorio_erbs_cod_mun;

CREATE INDEX idx_relatorio_erbs_cod_mun
  ON relatorio_erbs
  USING btree
  (cod_municipio_ibge COLLATE pg_catalog."default");

-- Table: relatorio_ranking

-- DROP TABLE relatorio_ranking;

CREATE TABLE relatorio_ranking
(
  uf character varying(2),
  cod_municipio_ibge character varying NOT NULL,
  municipio character varying,
  prestadora character varying NOT NULL,
  nome_fantasia character varying,
  qtd_tecnologia_2g integer,
  qtd_tecnologia_3g integer,
  qtd_tecnologia_4g integer,
  conexao_voz real,
  desconexao_voz real,
  conexao_dados real,
  desconexao_dados real,
  ranking_voz integer,
  ranking_dados integer,
  CONSTRAINT pk_relatorio_ranking PRIMARY KEY (cod_municipio_ibge, prestadora)
)
WITH (
  OIDS=FALSE
);
ALTER TABLE relatorio_ranking
  OWNER TO mobisaude_pg_user;

-- Index: idx_relatorio_ranking_cod_mun

-- DROP INDEX idx_relatorio_ranking_cod_mun;

CREATE INDEX idx_relatorio_ranking_cod_mun
  ON relatorio_ranking
  USING btree
  (cod_municipio_ibge COLLATE pg_catalog."default");

-- Table: token_sessao

-- DROP TABLE token_sessao;

CREATE TABLE token_sessao
(
  id_token_sessao integer NOT NULL,
  token character varying,
  validade timestamp without time zone,
  CONSTRAINT pk_token_sessao PRIMARY KEY (id_token_sessao)
)
WITH (
  OIDS=FALSE
);
ALTER TABLE token_sessao
  OWNER TO mobisaude_pg_user;
