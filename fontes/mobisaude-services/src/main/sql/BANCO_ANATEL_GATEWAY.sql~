-- Sequence: seq_historico

-- DROP SEQUENCE seq_historico;

CREATE SEQUENCE seq_historico
  INCREMENT 1
  MINVALUE 1
  MAXVALUE 9223372036854775807
  START 38
  CACHE 1;
ALTER TABLE seq_historico
  OWNER TO mobisaude_pg_user;

